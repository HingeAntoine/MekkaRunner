function jump_update()
 local ay_jump=0.35
 
 ymac += vymac
 vymac += ay_jump
 
 if ymac>100 then
  ymac=100
  vymac=0
  is_jumping=false
  is_landing=true
 end
end

function add_cell_bckgrnd()
 local id=flr(rnd(#bckgrnd_tiles))+1
 local cell=bckgrnd_tiles[id]
 add(bckgrnd,cell)
end

function obstacle_update()
 foreach(obstacles,function(obj)
  obj:move(vxmac) end)
 foreach(obstacles,
  function(obs)
   if obs.x+obs.obst.zone[1]*8 < 0 then
    del(obstacles,obs)
   end
  end)
end

function obstacle_draw()
  foreach(obstacles,function(obj)obj:draw() end)
  --foreach(obstacles,function(obj)obj:drawhit() end)
end

function obstaclefr_draw()
 foreach(obstacles,function(obj)
  obj:drawfr() end)
end

function bckgrnd_update()
 add_cell_bckgrnd()
 del(bckgrnd, bckgrnd[1])

 for i=1,bck_len do
  mset(i-1,3,bckgrnd[i])
 end
end

function check_collision()
 is_coll=false
 
 function check(part)
  foreach(obstacles,
   function(ob)
    part:collide(ob)
   end)
 end
 
 check(body.backleg)
 check(body.body)
 check(body.frontleg)
 foreach(body.mac,check)
end

function new_obstacles()
 local index=flr(rnd(#obst_sets))+1
 obstacles=obst_sets[index]:init()
end

