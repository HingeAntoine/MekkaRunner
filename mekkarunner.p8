pico-8 cartridge // http://www.pico-8.com
version 8
__lua__
--*******************
machinespr={8,{2,2}}
bodyspr={5,{2,3}}
frarmspr={7,{1,1}}
frlegspr={10,{1,3}}
bklegspr={11,{1,3}}
handspr={23,{1,1}}

forwardbklegspr={12,{2,2}}
forwardfrlegspr={14,{2,2}}


xmac=40
ymac=100

vxmac=0
vymac=0

is_jumping=false

one_frame=1/30
frame_count=0

--*******************
function jump_update()
 local ay_jump=0.125
 
 ymac += vymac
 vymac += ay_jump
 
 if ymac>100 then
  ymac=100
  vymac=0
  is_jumping=false
 end
end

--*******************
function draw_part(part,xc, yc)
 local s=part[1]
 local xl=part[2][1]
 local yl=part[2][2]
 
 spr(s,xc,yc,xl,yl)
end

function draw_machine(xrel,yrel)

 local ybody={yrel-17,yrel-16}
 local yfrarm={yrel-8,yrel-7}
 local yhand={yrel-6,yrel-5}
 local ymachine={yrel-8, yrel-7}
 
 local index = flr(frame_count/5)%2+1
 
 if index==2 then
  draw_part(forwardfrlegspr,0,0)
 end
 
 --draw_part(frarmspr,xrel+4,yrel-6)
 palt(0,false)
 palt(11,true)
 draw_part(bodyspr,xrel+1,ybody[index])
 palt(0,true)
 palt(11,false)
 draw_part(frarmspr,xrel,yfrarm[index])
 draw_part(bklegspr,xrel+6,yrel-1)
 draw_part(machinespr,xrel-1,ymachine[index])
 draw_part(handspr,xrel+4,yhand[index])
 draw_part(frlegspr,xrel+1,yrel-1)
end

--*******************
function _init()
 cls()
 rectfill(0,0,127,127,11)
end

function _draw()
 rectfill(0,0,127,127,11)
 draw_machine(xmac,ymac)
end

function _update()
 
 frame_count+=1
 
 if btn(0) then
  xmac-=1
 end
 
 if btn(1) then
  xmac+=1
 end
 
 if btnp(2) and not is_jumping then
  --ymac-=1
  is_jumping=true
  vymac=-2.5
 end
 
 if btn(3) then
  --ymac+=1
 end
 
 if is_jumping then
  jump_update()
 end

end
