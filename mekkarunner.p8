pico-8 cartridge // http://www.pico-8.com
version 8
__lua__
--*******************
machinespr={8,{2,2}}
bodyspr={5,{2,3}}
frarmspr={7,{1,1}}
handspr={23,{1,1}}

legspr={10,{1,3}}
forwardlegspr={13,{2,2}}
backwardlegspr={11,{2,3}}


xmac=40
ymac=100

vxmac=0
vymac=0

is_jumping=false

one_frame=1/30
frame_count=0

--*******************
function jump_update()
 local ay_jump=0.125
 
 ymac += vymac
 vymac += ay_jump
 
 if ymac>100 then
  ymac=100
  vymac=0
  is_jumping=false
 end
end

--*******************
function draw_part(part,xc, yc,bk)
 bk = bk or false
 local s=part[1]
 local xl=part[2][1]
 local yl=part[2][2]
 
 if bk then
  pal(9,5)
  pal(4,5)
 end
 
 spr(s,xc,yc,xl,yl)
 
 pal()
end

function draw_machine(xrel,yrel)

 local ybody={yrel-17,yrel-16}
 local yfrarm={yrel-8,yrel-7}
 local yhand={yrel-6,yrel-5}
 local ymachine={yrel-8, yrel-7}
 
 local index = flr(frame_count/5)%2+1
 local rid= flr(frame_count/10)%4
 
 if rid==0 then
  draw_part(backwardlegspr,xrel-3,yrel+1,true)
 elseif rid==1 or rid==3 then
  draw_part(legspr,xrel+6,yrel+1,true)
 elseif rid==2 then
  draw_part(forwardlegspr,xrel+6,yrel+1,true)
 end
 
 --draw_part(legspr,xrel+6,yrel-1,true)
 
 --draw_part(frarmspr,xrel+4,yrel-6)
 palt(0,false)
 palt(11,true)
 draw_part(bodyspr,xrel+1,ybody[index])
 palt(0,true)
 palt(11,false)
 
 draw_part(frarmspr,xrel,yfrarm[index])
 
 draw_part(machinespr,xrel-1,ymachine[index])
 draw_part(handspr,xrel+4,yhand[index])
 --draw_part(legspr,xrel+1,yrel-1)
 if rid==0 then
  draw_part(forwardlegspr,xrel+1,yrel+1)
 elseif rid==1 or rid==3 then
  draw_part(legspr,xrel+1,yrel+1)
 elseif rid==2 then
  draw_part(backwardlegspr,xrel-9,yrel+1)
 end

end

--*******************
function _init()
 cls()
 rectfill(0,0,127,127,11)
end

function _draw()
 rectfill(0,0,127,127,11)
 draw_machine(xmac,ymac)
end

function _update()
 
 frame_count+=1
 
 if btn(0) then
  xmac-=1
 end
 
 if btn(1) then
  xmac+=1
 end
 
 if btnp(2) and not is_jumping then
  --ymac-=1
  is_jumping=true
  vymac=-2.5
 end
 
 if btn(3) then
  --ymac+=1
 end
 
 if is_jumping then
  jump_update()
 end

end
