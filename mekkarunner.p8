function draw_background()
 --drawing floor
 local xstart=flr(frame_count*vxmac)%8*(-1)
 map(3,1, xstart,104, 17,3)

 --drawing background
 foreach(layers.second,
  function(obj)
   obj:draw()
  end)
 
 foreach(layers.first,
  function(obj)
   obj:draw()
  end)
 
end

function update_background()
 local xstart=flr(frame_count*vxmac)%8*(-1)
 obstacle_update()
 
 foreach(layers.second,
  function(obj)
   obj:update(0.5*vxmac)
   if obj.x<-20 then
    del(layers.second,obj)
   end
  end)
 
 foreach(layers.first,
  function(obj)
   obj:update(vxmac)
   if obj.x<-20 then
    del(layers.first,obj)
   end
  end)
  
 if #layers.second==0 then
  add_backgroundtree()
 end
 
 if xstart==0 then
  bckgrnd_update()
 end
end

function add_backgroundtree()
  local lay1=layer.create(150,
  {layer1.top[1],layer1.bot[1],
   layer1.midd[1],layer1.midd[2]})
 local lay2=layer.create(250,
  {layer1.top[2],layer1.bot[2],
   layer1.midd[1],layer1.midd[2]})
 local lay3=layer.create(350,
  {layer1.top[3],layer1.bot[3],
   layer1.midd[1],layer1.midd[2]})
 layers.first={lay1,lay2,lay3}
 
 local lay4=layer.create(150,
  {layer2.top[1],layer2.midd[1],layer2.bot[1]})
 local lay5=layer.create(200,
  {layer2.top[1],layer2.midd[1],layer2.bot[1]})
 layers.second={lay4,lay5}
end

function draw_machine()
 --draw back leg
 pal(9,5)
 pal(4,5)
 body.backleg:draw()
 pal()

 --draw body
 palt(0,false)
 palt(11,true)
 body.body:draw()
 palt(0,true)
 palt(11,false)
 
 --draw rest
 foreach(body.mac,
  function(obj)
   obj:draw()
  end)
 
 --draw front leg
 body.frontleg:draw()
 
 --draw hitboxes
 local drawhit = false
 if drawhit then
  body.body:drawhit()
  foreach(body.mac,
   function(obj)
    obj:drawhit()
   end)
  body.backleg:drawhit()
  body.frontleg:drawhit()
 end
end

function update_legs()
 local index= flr(sprite_count/10)%4

 if index==0 then
  body.backleg=real_obst.create(
   xmac-3,ymac+1,backwardlego)
  body.frontleg=real_obst.create(
   xmac+1,ymac+1,forwardlego)
 elseif index==1 or index==3 then
  body.backleg=real_obst.create(
   xmac+6,ymac+1,lego)
  body.frontleg=real_obst.create(
   xmac+1,ymac+1,lego)
 elseif index==2 then
  body.backleg=real_obst.create(
   xmac+6,ymac+1,forwardlego)
  body.frontleg=real_obst.create(
   xmac-9,ymac+1,backwardlego)
 end
 
end

function update_machine()
 if sprite_count%(3*vxmac)==0 then
  local index=flr(sprite_count/6)%2
 
  if index==0 then
   create_body(xmac,ymac)
  else
   create_body(xmac,ymac-1)
  end
 end
 
 if sprite_count%(5*vxmac)==0 and not jump then
  update_legs()
 end
end

function update_mac_jump()
 function jump_config()
  create_body(xmac,ymac+2)
  body.backleg=real_obst.create(
   xmac+6,ymac+1,lego)
  body.frontleg=real_obst.create(
   xmac+1,ymac+1,lego)
 end  

 if is_launching then
  launch_count=max(launch_count,0)+1
  
  if btn(2) then
   vymac -= 0.1
   launch_thresh=min(launch_thresh+1,10)
  end
  
  if launch_count==1 then
   jump_config()
  elseif launch_count>launch_thresh then
   is_launching=false
   is_jumping=true
   landing_count=0
   launch_count=0
   launch_thresh=threshval
  end
  
  return
 end
 
 if is_jumping then
  create_body(xmac,ymac)
  body.backleg=real_obst.create(
    xmac-3,ymac+1,backwardlego)
  body.frontleg=real_obst.create(
    xmac+1,ymac+1,forwardlego)
  return
 end
 
 if is_landing then
  landing_count=max(landing_count,0)+1
 
  if landing_count==1 then
   jump_config()
  elseif landing_count>5 then
   sprite_count=14
   is_landing=false
   landing_count=0
   create_body(xmac,ymac)
  end

  return
 end
end

function update_mac_slide() 
 create_body(xmac,ymac+14)
 body.backleg=real_obst.create(
  xmac-13,ymac+14,leglying)
 body.frontleg=real_obst.create(
  xmac+1,ymac+14,leglyingf)
 is_sliding=false
 sprite_count=14
end

function animate_fall()
 body.mac={}
 
 if ymac > 100 then
  body.body=real_obst.create(
   xmac,ymac,lyingbody)
  
  body.mac={}
  add(body.mac,real_obst.create(
   xmac*0.72,ymac-6,machineo))
  add(body.mac,real_obst.create(
   xmac+16,ymac+2,armfall))
  add(body.mac,real_obst.create(
   xmac+22,ymac+4,hando))
  
  body.frontleg=real_obst.create(
   xmac*0.8-26,ymac+2,leglying)
  body.backleg=real_obst.create(
   xmac*0.8-24,ymac+2,leglying)
  return
 else
  body.body=real_obst.create(
   xmac,ymac,fallingbody)
  
  body.mac={}
  add(body.mac,real_obst.create(
   xmac*0.8,ymac,machinefall))
  add(body.mac,real_obst.create(
   xmac+17,ymac+8,armfall))
  add(body.mac,real_obst.create(
   xmac+22,ymac+10,hando))
  
  body.frontleg=real_obst.create(
   xmac*0.8-9,ymac+8,backwardlego)
  body.backleg=real_obst.create(
   xmac*0.8-7,ymac+8,backwardlego)
  return
 end
end

function fall_update()
 local ay_fall=0.35
 
 ymac  += vymac
 vymac += ay_fall
 
 xmac  += vxfall
 
 if ymac>112 then
  ymac=112
  vymac=0
  local ax_gnd=0.7
  vxfall*=ax_gnd
  if vxfall <0.001 then
   vxfall=0
  end
 end
end

