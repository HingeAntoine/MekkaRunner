ymac=100

--*******************
function draw_part(part,xc, yc)
 local s=part[1]
 local xl=part[2][1]
 local yl=part[2][2]
 
 spr(s,xc,yc,xl,yl)
end

function draw_machine(xrel,yrel)
 --local xrel=40
 --local yrel=100

 --draw_part(frarmspr,xrel+4,yrel-6)
 palt(0,false)
 palt(11,true)
 draw_part(bodyspr,xrel+1,yrel-17)
 palt(0,true)
 palt(11,false)
 draw_part(frarmspr,xrel,yrel-8)
 draw_part(bklegspr,xrel+6,yrel-1)
 draw_part(machinespr,xrel-1,yrel-8)
 draw_part(handspr,xrel+4,yrel-6)
 draw_part(frlegspr,xrel+1,yrel-1)
end

--*******************
function _init()
 cls()
 rectfill(0,0,127,127,11)
end

function _draw()
 rectfill(0,0,127,127,11)
 draw_machine(xmac,ymac)
end

function _update()
 
 if btn(0) then
  xmac-=1
 elseif btn(1) then
  xmac+=1
 elseif btn(2) then
  ymac-=1
 elseif btn(3) then
  ymac+=1
 end

end
