function create_body(x,y) 
 local bodypart=real_obst.create(
  x+1,y-17,bodyo)
 body.body=bodypart

 local machine=real_obst.create(
  x-1,y-8,machineo)
 local arm   =real_obst.create(
  x,y-8,armo)
 local hand   =real_obst.create(
  x+4,y-6,hando)
 body.mac ={arm,machine,hand}
end

function create_legs(x,y)
 local legbck=real_obst.create(
  x+6,y+1,lego)
 local legfr=real_obst.create(
  x+1,y+1,lego)
 body.backleg=legbck
 body.frontleg=legfr
end

function create_machine()
 create_body(xmac,ymac)
 create_legs(xmac,ymac)
end

function draw_moon()
 --frame_count
 local v  = 25
 local t  = frame_count/v
 local xc = t
 local yc = 50/(64*64)*xc*(xc-128)+120
 
 --print("xc:"..xc,0,40,10)
 --print("yc:"..yc,0,50,10)
 
 --circfill(108,20,10,6)
 circfill(xc,yc,10,6)
 circfill(xc-1,yc-1,8,5)
end

