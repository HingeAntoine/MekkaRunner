--******** hitbox mgmt ********
hitbox={}
hitbox.__index=hitbox

function hitbox.create(x1,y1,x2,y2)
 local htbx={}
 setmetatable(htbx,hitbox)
 htbx.x1=x1
 htbx.x2=x2
 htbx.y1=y1
 htbx.y2=y2
 return htbx
end

function hitbox:translate(x,y)
 local htbx={}
 setmetatable(htbx,hitbox)
 htbx.x1=self.x1+x
 htbx.x2=self.x2+x
 htbx.y1=self.y1+y
 htbx.y2=self.y2+y
 return htbx
end

function hitbox:draw(x,y)
 rect(x+self.x1,y+self.y1,
  x+self.x2,y+self.y2,8)
end

function hitbox:collide(htbx)
 local res=self.x2 < htbx.x1
  or self.x1 > htbx.x2
  or self.y2 < htbx.y1
  or self.y1 > htbx.y2
 is_coll=(not res) or is_coll
end

--******* obstacle mgmt *******
obstacle={}
obstacle.__index=obstacle

function obstacle.create(sprite,zone,htbxs)
 local obs={}
 setmetatable(obs,obstacle)
 --back sprite
 obs.sprite=sprite
 obs.zone=zone
 obs.hitboxes=htbxs
 --front sprite
 obs.spritefr=0
 obs.zonefr={1,1}
 obs.xoff=0
 obs.yoff=0
 return obs
end

function obstacle:draw(x,y)
 spr(self.sprite,x,y,
  self.zone[1],self.zone[2])
end

function obstacle:drawhit(x,y)
 foreach(self.hitboxes, function(obj) obj:draw(x,y) end)
end

function obstacle:drawfr(x,y)
 if self.spritefr == 0 then
  return
 end
 spr(self.spritefr,
  x+self.xoff,y+self.yoff,
  self.zonefr[1],self.zonefr[2])
end

--******* real_obst mgmt *******
real_obst={}
real_obst.__index=real_obst

function real_obst.create(x,y,obst)
 local obj={}
 setmetatable(obj,real_obst)
 obj.x=x
 obj.y=y
 obj.obst=obst
 obj.xfr=0
 obj.yfr=0
 return obj
end

function real_obst:move(x)
 self.x-=x
end

function real_obst:draw()
 self.obst:draw(self.x,self.y)
end

function real_obst:drawhit()
 self.obst:drawhit(self.x,self.y)
end

function real_obst:drawfr()
 self.obst:drawfr(self.x,self.y)
end

function real_obst:collide(obst)
 foreach(self.obst.hitboxes,
  function(hita)
   if is_coll then
    return
   end
   local h1=hita:translate(self.x,self.y)
   foreach(obst.obst.hitboxes,
    function(hitb)
     if is_coll then
      return
     end
     local h2=hitb:translate(obst.x,obst.y)
     h1:collide(h2)
    end)
  end)
end

--****** layerpart mgmt ******
layerpart={}
layerpart.__index=layerpart

function layerpart.create(sprite,y,size)
 local obj={}
 setmetatable(obj,layerpart)
 obj.sprite=sprite
 obj.y=y
 obj.size=size
 return obj
end

function layerpart:drawpart(x)
 spr(self.sprite,
  x,self.y,
  self.size[1],self.size[2])
end

--********* layer mgmt ********
layer={}
layer.__index=layer

function layer.create(x,layerparts)
 local obj={}
 setmetatable(obj,layer)
 obj.parts=layerparts
 obj.x=x
 obj.radius=12
 return obj
end

function layer:draw()

 circfill(
  self.x+5,self:miny()-3,
  self.radius,3)

 foreach(self.parts,
  function(obj)
   obj:drawpart(self.x)
  end)
end

function layer:miny()
 local y=200
 foreach(self.parts,
  function(obj)
   y=min(obj.y,y)
  end)
 return y
end

function layer:update(x)
 self.x-=x
end

--******** obstset mgmt ******
obstset={}
obstset.__index=obstset

function obstset.create(xset,yset,obsts)
 local obj={}
 setmetatable(obj,obstset)
 obj.obsts=obsts
 obj.xset=xset
 obj.yset=yset
 return obj
end

function obstset:init()
 local set={}
 for i=1,#self.obsts do
  local obst=real_obst.create(
   self.xset[i],self.yset[i],
   obst_tiles[self.obsts[i]])
  add(set, obst)
 end
 return set
end

